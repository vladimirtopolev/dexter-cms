{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "noImplicitAny": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "allowJs": true,
    "lib": [
      "es2017",
      "dom"
    ],
    "outDir": "../build/server",
    "resolveJsonModule": true
  }
}
